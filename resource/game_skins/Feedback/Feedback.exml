			</e:Group>
			<e:Group width="400" height="150" anchorOffsetX="0" anchorOffsetY="0" top="110" horizontalCenter="0">
				<e:Image width="100%" height="150" source="trans_bg_png" horizontalCenter="0"  anchorOffsetY="0" anchorOffsetX="0" scale9Grid="28,6,173,38" verticalCenter="0"/>
				<e:Label id="labContent" text="来自:" fontFamily="微软雅黑" textAlign="left" width="95%" size="20"  height="150" verticalAlign="top" anchorOffsetX="0" horizontalCenter="1.5" anchorOffsetY="0" verticalCenter="0.5" textColor="0xffffff" multiline="true" wordWrap="true"/>
			</e:Group>
			<e:Label text="答复时间:" fontFamily="微软雅黑" textAlign="center" width="100" size="20"  height="35" verticalAlign="middle" anchorOffsetX="0" top="275" left="0" textColor="0xcec7b3"/>
			<e:Group width="340" height="50" anchorOffsetX="0" anchorOffsetY="0" top="270" left="100">
				<e:Image width="100%" height="100%" source="txt_bg_png" horizontalCenter="0" verticalCenter="0"/>
				<e:Label id="labResponseDate" text="来自:" fontFamily="微软雅黑" textAlign="left" width="95%" size="20"  height="90%" verticalAlign="middle" anchorOffsetX="0" horizontalCenter="1.5" anchorOffsetY="0" verticalCenter="0.5" textColor="0xffffff"/>
			</e:Group>
			<e:Group width="400" height="150" anchorOffsetX="0" anchorOffsetY="0" top="330" horizontalCenter="0">
				<e:Image width="100%" height="150" source="trans_bg_png" horizontalCenter="0"  anchorOffsetY="0" anchorOffsetX="0" scale9Grid="28,6,173,38" y="0"/>
				<e:Label id="labResponseContent" text="来自:" fontFamily="微软雅黑" textAlign="left" width="95%" size="20"  height="150" verticalAlign="top" anchorOffsetX="0" horizontalCenter="1.5" anchorOffsetY="0" verticalCenter="0.5" textColor="0xffffff" multiline="true" wordWrap="true"/>
			</e:Group>
		</e:Group>
